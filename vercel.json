{
    "routes": [
        {
            "src": "/api",
            "dest": "/api/run.go"
        },
        {
            "src": "/search",
            "dest": "/search.html"
        },
        {
            "src": "/search_do",
            "dest": "/api/run.go"
        },
        {
            "src": "/login",
            "dest": "/login.html"
        },
        {
            "src": "/login_do",
            "dest": "/api/run.go"
        },
        {
            "src": "/check_token",
            "dest": "/api/run.go"
        },
        {
            "src": "/result",
            "dest": "/result.html"
        },
        {
            "src": "/cache",
            "dest": "/api/cache.go"
        }
    ],
    "env": {
        "JWT_EXPIRES_AT": "12",
        "CACHE_EXPIRATION_AT": "24"
    }
}
