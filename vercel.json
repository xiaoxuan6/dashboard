{
    "routes": [
        {
            "src": "/api",
            "dest": "/api/run.go"
        },
        {
            "src": "/apis/(.*)",
            "dest": "/api/api.go"
        },
        {
            "src": "/wechat",
            "dest": "/api/wechat.go"
        },
        {
            "src": "/search",
            "dest": "/search.html"
        },
        {
            "src": "/login",
            "dest": "/login.html"
        },
        {
            "src": "/result",
            "dest": "/result.html"
        }
    ],
    "env": {
        "JWT_EXPIRES_AT": "12",
        "GITHUB_OWNER": "xiaoxuan6",
        "GITHUB_REPO": "resource"
    }
}
