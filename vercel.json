{
    "routes": [
        {
            "src": "/api",
            "dest": "/api/run.go"
        },
        {
            "src": "/apis",
            "dest": "/api/api.go"
        },
        {
            "src": "/apis/index",
            "dest": "/apis_index.html"
        },
        {
            "src": "/apis/docs/index",
            "dest": "/apis_docs.html"
        },
        {
            "src": "/apis/(?!(index$|docs/index$))(.*)",
            "dest": "/api/api.go"
        },
        {
            "src": "/wechat",
            "dest": "/api/wechat.go"
        },
        {
            "src": "/search",
            "dest": "/search.html"
        },
        {
            "src": "/login",
            "dest": "/login.html"
        }
    ],
    "env": {
        "JWT_EXPIRES_AT": "72",
        "GITHUB_OWNER": "xiaoxuan6",
        "GITHUB_REPO": "resource"
    }
}
